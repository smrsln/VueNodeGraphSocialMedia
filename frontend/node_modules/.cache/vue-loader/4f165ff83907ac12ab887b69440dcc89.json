piService.likeService(item).then(data => {\r\n        //console.log(\"data bu \" + JSON.stringify(data));\r\n        if (data.data === \"ok\") {\r\n          this.$store.dispatch(\"setCurrentTitle\", {\r\n            id: this.$store.state.currentTitle.id\r\n          });\r\n        } else {\r\n          console.log(\"else çalıştı\");\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}